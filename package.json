{
  "name": "nua.ge-website",
  "private": true,
  "description": "Nua.ge website",
  "version": "1.0.0",
  "license": "unlicensed",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby telemetry --disable && gatsby develop --host 0.0.0.0 --port ${PORT:-8443}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "start": "npm run develop",
    "serve": "gatsby telemetry --disable && gatsby serve --host 0.0.0.0 --port ${PORT:-8443}",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@creativebulma/bulma-collapsible": "^1.0.4",
    "apollo-boost": "^0.4.4",
    "axios": "^0.19.0",
    "bulma": "^0.8.0",
    "esm": "^3.2.25",
    "gatsby": "^2.4.3",
    "gatsby-plugin-manifest": "^2.2.29",
    "gatsby-plugin-netlify-cms": "^4.1.28",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.1.23",
    "gatsby-plugin-sitemap": "^2.2.21",
    "gatsby-plugin-typescript": "^2.0.13",
    "gatsby-remark-source-name": "^1.0.0",
    "gatsby-source-filesystem": "^2.1.37",
    "gatsby-transformer-remark": "^2.6.36",
    "graphql-tag": "^2.10.1",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.24.0",
    "netlify-cms-app": "^2.9.7",
    "node-sass": "^4.13.0",
    "react": "^16.8.6",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "remark-react": "^6.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jsdom": "^9.4.0",
    "lint-staged": "^8.1.6",
    "path": "^0.12.7",
    "prettier": "^1.17.0",
    "typescript": "^3.4.5"
  }
}
